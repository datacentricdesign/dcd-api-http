{
  "name": "dcd-api-http",
  "version": "0.1.0-dev",
  "private": false,
  "description": "The HTTP API of the Data-Centric Design Hub",
  "homepage": "https://github.com/datacentricdesign/dcd-api-http#README",
  "bugs": {
    "url": "https://github.com/datacentricdesign/dcd-api-http/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datacentricdesign/dcd-api-http.git"
  },
  "license": "MIT",
  "author": "Jacky Bourgeois",
  "main": "index.js",
  "scripts": {
    "clean": "remark .",
    "doc": "apidoc -i routes/ -o public/docs/",
    "production": "npm run doc && npm start",
    "start": "node index.js",
    "test": "nyc --reporter=html --reporter=text mocha --exit"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "dependencies": {
    "@datacentricdesign/model": "0.1.8-dev3",
    "apidoc": "0.17.7",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "express-basic-auth": "1.2.0",
    "express-jwt": "5.3.1",
    "jwks-rsa": "1.6.0",
    "log4js": "5.0.0",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "multiparty": "4.2.1",
    "node-fetch": "2.6.0",
    "serve-favicon": "2.5.0",
    "simple-oauth2": "2.2.1",
    "swagger-stats": "0.95.11"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^6.3.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "remark-cli": "^8.0.0",
    "remark-lint": "^7.0.0",
    "remark-preset-lint-recommended": "^4.0.0"
  }
}
